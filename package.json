{
  "name": "sonea",
  "version": "0.1.0",
  "description": "web framework",
  "preferGlobal": true,
  "main": "./lib/index.js",
  "bin": {
    "sonea": "./bin/sonea"
  },
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "conventional-changelog-lint -e",
    "precommit": "npm run lint",
    "lint": "eslint lib test",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha test",
    "test:coverage": "istanbul cover _mocha test"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/naxmefy/sonea.git"
  },
  "keywords": [
    "sonea",
    "web framework"
  ],
  "author": {
    "name": "MRW Neundorf [naxmefy]",
    "email": "m.neundorf@live.de",
    "url": "http://www.nax.me"
  },
  "bugs": {
    "url": "https://github.com/naxmefy/sonea/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/naxmefy/sonea",
  "engines": {
    "node": ">=6.9.1"
  },
  "devDependencies": {
    "commitizen": "2.8.6",
    "conventional-changelog-lint": "1.1.0",
    "conventional-changelog-lint-config-angular": "0.3.0",
    "cz-customizable": "4.0.0",
    "eslint": "3.10.1",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-promise": "3.3.2",
    "eslint-plugin-standard": "2.0.1",
    "husky": "0.11.9",
    "istanbul": "0.4.5",
    "mocha": "2.5.3",
    "should": "9.0.0"
  },
  "dependencies": {
    "async": "2.0.0-rc.5",
    "debug": "2.2.0",
    "express": "4.13.4",
    "lodash": "4.13.1",
    "pug": "2.0.0-beta2"
  }
}
